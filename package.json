    "express-session": "^1.15.6",
    "fs": "^0.0.1-security",
    "if-env": "^1.0.4",
    "mongoose": "^5.0.17",
    "mysql2": "^1.6.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
